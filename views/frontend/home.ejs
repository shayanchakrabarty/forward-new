  <% include ./partials/header %>
  <!-- banner -->
  <section class="banner-sec">
    <div class="sgl-pro-outr">
      <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Wrapper for slides -->
        <div class="carousel-inner">
          <div class="statickbanner"><img src="frontend/images/home-banner.jpg"></div>
          <% slider.forEach(function(slide, index) { %>
          <div class="item <% if(index == 0) {%> active<% } %>">
            <!-- <img src="frontend/images/home-banner.jpg" style="width:100%;"> -->
            <div class="intro-text">
              <div class="container">
                <div class="intro-lead-in wow fadeInLeft animated"><%= slide.slider_first_text %></div>
                <div class="intro-heading wow fadeInRight animated" data-wow-delay="0.2s"><%= slide.slider_second_text %></div>
                <div class="intro-foo wow fadeInLeft animated" data-wow-delay="0.4s"><%= slide.slider_third_text %></div>
              </div>
            </div>
          </div>
          <% }); %>
        </div>
      </div>
    </div>
  </section>
