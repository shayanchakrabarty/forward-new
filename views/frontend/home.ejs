  <section id="contact-sec" class="commn_outr">
    <div class="container">
      <div class="col-lg-6 col-sm-6 col-xs-6 text-left">
        <div class="contact-head wow fadeInDown animated" data-wow-delay="0.4s">
          <h3>Can’t find what you want? Let us call you back</h3>
        </div>
        <div class="contactform_body company_logo_txt wow fadeInLeft" data-wow-delay="0.4s">
          <% if(errors) { %>
              <% errors.forEach(function(error){ %>
                <div class="fga alert alert-danger">
                  <%= error.msg %>
                </div>
              <% }); %>
          <% } %>
          <form action="/contact-us" method="post">
            <div class="col-sm-12 col-md-12">
              <div class="contact-form">
                <input type="text" name="contact_name" placeholder="Name">
              </div>
            </div>
            <div class="col-sm-12 col-md-12">
              <div class="contact-form">
                <input type="phone" name="contact_phone" placeholder="Phone">
              </div>
            </div>
            <div class="col-sm-12 col-md-12">
              <div class="contact-form">
                <textarea name="contact_message" placeholder="What this is About?"></textarea>
              </div>
            </div>
            <div class="col-sm-12 col-md-12">
              <div class="contact-form">
                <p class="sdf">Jack or Rooney will call you back within 24 hours.</p>
              </div>
            </div>
            <div class="col-md-12 col-sm-12" style="margin-top: 15px;">
              <!-- <a href="#" class="button buy-now sendbtn contform-btm">Request a Call Back</a> -->
              <input type="submit" class="button buy-now sendbtn contform-btm" value="Request a Call Back">
            </div>
             
          </form>
           
        </div>
          <div class="conti">
